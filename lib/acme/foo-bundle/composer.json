{
  "name": "acme/foo-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle for ...",
  "keywords": ["foo", "bar"],
  "homepage": "https://github.com/acme/AcmeFooBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Your name",
      "email": "Your email"
    }
  ],
  "require": {
    "php": ">=7.2",
    "symfony/framework-bundle": ">=4.0",
    "symfony/yaml": ">=4.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge": ">=4.0"
  },
  "config": {
    "platform": {
      "php": "7.2"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Acme\\FooBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Acme\\FooBundle\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "<4.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}